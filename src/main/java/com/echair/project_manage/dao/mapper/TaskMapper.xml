  <select id="getTaskProjectList" resultMap="UserProjectVOMap">
  select p.project_id,p.project_name,count(t.task_id) as size,p.rank from
  project p
  inner join  participation pa ON pa.project_id = p.project_id and pa.userid =#{userid} AND pa.is_del = 0 AND pa.is_executed = 1
  left join task t on t.project_id = p.project_id and t.execute_userid = pa.userid and t.is_del = 0  and t.begin = #{begin}
  and t.end = #{end}
  where
  p.is_del = 0
  group by p.project_id
  order by p.create_time desc
</select>
